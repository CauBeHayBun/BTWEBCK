							values = values.split(',');
							if(is_color){
								variantSwatch += '<div data-value="'+_value+'" class="n-sd swatch-element color '+ value;

							}else{
								variantSwatch += '<div data-value="'+_value+'" class="n-sd swatch-element '+ value;
